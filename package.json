{
  "name": "gno-to-ged",
  "version": "0.1.1",
  "description": "GenoPro .gno to GEDCOM .ged converter (TypeScript)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "gno-to-ged": "dist/cli-gno-to-ged.js",
    "ged-to-gno": "dist/cli-ged-to-gno.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev:gno": "tsx src/cli-gno-to-ged.ts",
    "dev:ged": "tsx src/cli-ged-to-gno.ts"
  },
  "dependencies": {
    "fast-xml-parser": "^4.4.1",
    "pako": "^2.1.0",
    "unzipper": "^0.12.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/pako": "^2.0.4",
    "@types/unzipper": "^0.10.11",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
