{
  "name": "@treeviz/gno-to-ged",
  "version": "1.0.13",
  "description": "GenoPro .gno to GEDCOM .ged converter (TypeScript)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "gno-to-ged": "dist/cli-gno-to-ged.js",
    "ged-to-gno": "dist/cli-ged-to-gno.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "dev:gno": "tsx src/cli-gno-to-ged.ts",
    "dev:ged": "tsx src/cli-ged-to-gno.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:idavidka/gno-to-ged.git"
  },
  "author": "idavidka & @treeviz contributors",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "genopro",
    "gno",
    "gedcom",
    "ged",
    "converter",
    "genealogy",
    "family-tree",
    "typescript"
  ],
  "dependencies": {
    "fast-xml-parser": "^4.4.1",
    "jszip": "^3.10.1",
    "pako": "^2.1.0",
    "yargs": "^17.7.2"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/pako": "^2.0.4",
    "@types/unzipper": "^0.10.11",
    "@types/yargs": "^17.0.33",
    "tsup": "^8.5.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^3.0.7"
  }
}
